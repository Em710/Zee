def menu():
	os.system('clear')
	try:
		toket=open('login.txt','r').read()
	except IOError:
		os.system('clear')
		print"\033[1;96m[!] \x1b[1;91mToken invalid"
		os.system('rm -rf login.txt')
		time.sleep(1)
		keluar()
	try:
		otw = requests.get('https://graph.facebook.com/me?access_token='+toket)
		a = json.loads(otw.text)
		nama = a['name']
		id = a['id']
		ots = requests.get('https://graph.facebook.com/me/subscribers?access_token=' + toket)
		b = json.loads(ots.text)
		sub = str(b['summary']['total_count'])
	except KeyError:
		os.system('clear')
		print"\033[1;96m[!] \033[1;91mToken invalid"
		os.system('rm -rf login.txt')
		time.sleep(1)
		login()
	except requests.exceptions.ConnectionError:
		print"\033[1;96m[!] \x1b[1;91mConnection Error"
		keluar()
	os.system("clear")
	print logo
	print 42*"\033[1;97m═"
	print "[•] Name:        "+nama+" "                               
	print "[•] ID  :        "+id+"   "
	print "[•] Subs:        "+sub+"  "
	print 42*"\033[1;97m═"
	print "[1] Crack With Facebook"
	print "[2] Crack With ZeDD"
	print "[3] Crack With Fast"
	print "[4] Fuck You Haterz?"
	print "[0] Exit The Program"
	pilih()
 
 
def pilih():
	unikers = raw_input("\n⌯⌯⌯⌯⌯⋙ ")
	if unikers =="":
		print ("[!] Fill in Correctly")
		pilih()
	elif unikers =="1":
		super()
	elif unikers =="2":
		super()
	elif unikers =="3":
		super()
	elif unikers =="4":
		super()
	elif unikers =="0":
		os.system('clear')
		jalan('Token Removed')
		os.system('rm -rf login.txt')
		keluar()
	else:
		print ("[!] Fill in Correctly")
		pilih()
		
		
def super():
	global toket
	os.system('clear')
	try:
		toket=open('login.txt','r').read()
