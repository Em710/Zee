			print"[!] \x1b[1;91mTarget No Found"
			raw_input("\n[Back]")
			super()
		jalan('[✺] Getting IDz\033[1;97m...')
		r = requests.get("https://graph.facebook.com/"+idt+"/friends?access_token="+toket)
		z = json.loads(r.text)
		for i in z['data']:
			id.append(i['id'])
        elif peak =="3":
		os.system('clear')
		print logo
